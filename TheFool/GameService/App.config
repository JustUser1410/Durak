<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <services>
      <service name="GameService.Program">
        <endpoint address="http://localhost:8888/GameService/" binding="wsDualHttpBinding"
          contract="GameContract.IGame">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8888/GameService/" />
          </baseAddresses>
        </host>
      </service>
    </services>
  </system.serviceModel>
  
  
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
</configuration>